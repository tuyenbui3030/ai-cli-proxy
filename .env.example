# CLIProxyAPI Configuration
# Copy this file to .env and modify as needed

# Server port (default: 8317)
PORT=8317

# Timezone
TZ=Asia/Ho_Chi_Minh

# Watchtower auto-update interval in seconds (default: 3600 = 1 hour)
UPDATE_INTERVAL=3600

# API Key for model trigger (warmup at 7am)
TRIGGER_API_KEY=sk-change-this-api-key
